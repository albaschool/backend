{
  "openapi": "3.1.1",
  "info": {
    "version": "1.0.0",
    "title": "REST API",
    "description": ""
  },
  "servers": [
    {
      "url": "http://localhost:3000/"
    }
  ],
  "tags": [
    {
      "name": "Stores",
      "description": "가게 엔드포인트"
    }
  ],
  "paths": {},
  "components": {
    "schemas": {
      "createStore": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "example": "GS25 서울역점"
          },
          "location": {
            "type": "string",
            "example": "서울 용산구 한강대로 401"
          },
          "contact": {
            "type": "string",
            "example": "0212345678"
          },
          "password": {
            "type": "string",
            "example": "password"
          }
        },
        "required": [
          "title",
          "location",
          "contact",
          "password"
        ]
      },
      "addMember": {
        "type": "object",
        "properties": {
          "memberId": {
            "type": "string",
            "example": ""
          }
        },
        "required": [
          "memberId"
        ]
      },
      "emailSend": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "example@mail.com"
          }
        }
      },
      "emailVerify": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "example@mail.com"
          },
          "code": {
            "type": "string",
            "example": "00000"
          }
        }
      },
      "saveUser": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "김철수"
          },
          "email": {
            "type": "string",
            "example": "example@mail.com"
          },
          "role": {
            "type": "string",
            "example": "staff"
          },
          "password": {
            "type": "string",
            "example": "0000"
          },
          "contact": {
            "type": "string",
            "example": "01012341234"
          }
        }
      },
      "password": {
        "type": "object",
        "properties": {
          "password": {
            "type": "string",
            "example": "0000"
          }
        }
      }
    },
    "examples": {
      "storesExample": {
        "value": [
          {
            "id": "4uDfVBHq",
            "title": "GS25 서울역점",
            "location": "서울 용산구 한강대로 401"
          }
        ]
      },
      "storeExample": {
        "value": {
          "title": "GS25 서울역점",
          "location": "서울 용산구 한강대로 401",
          "contact": "0212345678"
        }
      }
    },
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    }
  }
}